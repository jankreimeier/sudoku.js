/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "esModuleInterop": true,
    "outDir": "./dist",
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "experimentalDecorators": false,
    "moduleResolution": "node",
    "importHelpers": false,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "dom"],
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}
